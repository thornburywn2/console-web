{
  "name": "console-web",
  "version": "1.0.23",
  "description": "Console.web - Web-based management interface for Claude Code projects with terminal access and system monitoring",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "node --watch server/index.js",
    "dev:client": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server/index.js",
    "start:prod": "NODE_ENV=production node server/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:server": "vitest run --config vitest.config.server.js",
    "test:server:watch": "vitest --config vitest.config.server.js",
    "test:server:coverage": "vitest run --config vitest.config.server.js --coverage",
    "test:all": "vitest run && vitest run --config vitest.config.server.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "sanitize": "bash scripts/sanitize-push.sh",
    "sanitize:verbose": "bash scripts/sanitize-push.sh --verbose",
    "sanitize:fix": "bash scripts/sanitize-push.sh --fix",
    "setup:hooks": "bash scripts/setup-hooks.sh",
    "prepush": "npm run sanitize",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint src server --max-warnings 0",
    "lint:fix": "eslint src server --fix"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@octokit/rest": "^22.0.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.68.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.210.0",
    "@opentelemetry/sdk-node": "^0.210.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@sentry/node": "^10.34.0",
    "@sentry/react": "^10.34.0",
    "@xterm/addon-clipboard": "^0.2.0",
    "@xterm/addon-serialize": "^0.14.0",
    "@xterm/addon-unicode11": "^0.9.0",
    "chart.js": "^4.5.1",
    "chokidar": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dockerode": "^4.0.9",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-rate-limit": "^8.2.1",
    "fuse.js": "^7.1.0",
    "helmet": "^8.1.0",
    "howler": "^2.2.4",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.562.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "node-pty": "^1.0.0",
    "pg": "^8.16.3",
    "pino": "^10.2.0",
    "pino-pretty": "^13.1.3",
    "prisma": "^7.2.0",
    "prismjs": "^1.30.0",
    "prom-client": "^15.1.3",
    "react-chartjs-2": "^5.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-markdown": "^10.1.0",
    "socket.io": "^4.7.5",
    "uuid": "^13.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.0",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-onboarding": "^10.1.11",
    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^17.0.0",
    "jsdom": "^26.0.0",
    "playwright": "^1.57.0",
    "postcss": "^8.4.41",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io-client": "^4.7.5",
    "storybook": "^10.1.11",
    "supertest": "^7.2.2",
    "tailwindcss": "^3.4.10",
    "vite": "^5.4.21",
    "vitest": "^3.2.4",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
